{
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "https://tasks-api.assisjuniorwm.com.br/$1",
        "headers": {
          "Access-Control-Allow-Origin": "https://tasks.assisjuniorwm.com.br"
        }
      },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  